			</sdg:UserIdentityAttribute>
						
			<sdg:UserIdentityAttribute name="altro_attributo">
				<sdg:Label lang="it" >altro attributo</sdg:Label>
				<sdg:Label lang="en" >other attribute</sdg:Label>
			</sdg:UserIdentityAttribute>
						
          </sdg:DataServiceEvidenceType>
        </rim:SlotValue>
