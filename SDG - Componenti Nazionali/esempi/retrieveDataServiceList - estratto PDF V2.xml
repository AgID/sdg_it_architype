                  <sdg:PostCode>22083</sdg:PostCode>
                  <sdg:PostCityName>Hamburg</sdg:PostCityName>
                  <sdg:AdminUnitLevel1>DE</sdg:AdminUnitLevel1>
                  <sdg:AdminUnitLevel2>DE6</sdg:AdminUnitLevel2>
